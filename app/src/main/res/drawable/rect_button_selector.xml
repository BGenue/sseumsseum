<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false"
        android:state_pressed="false"
        android:drawable="@drawable/no_num_button_unpressed"/>
    <item android:state_enabled="false"
        android:state_pressed="true"
        android:drawable="@drawable/no_num_button_pressed"/>
    <item android:state_enabled="true"
        android:state_pressed="false"
        android:drawable="@drawable/has_num_button_unpressed"/>
    <item android:state_enabled="true"
        android:state_pressed="true"
        android:drawable="@drawable/has_num_button_pressed_ripple"/>
</selector>